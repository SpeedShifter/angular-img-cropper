{
  "name": "angular-img-cropper",
  "version": "1.1.1",
  "description": "Client side image cropper directive for AngularJS (rectangular area, aspect ratio, multi-touch)",
  "scripts": {
    "dev": "npm run build && live-server example/",
    "build": "jslint src/* && npm run js:bundle && npm run js:min",
    "clean": "rm -rf dist/*",
    "jslint": "jslint src/* || true",
    "js:bundle": "browserify src/angular-img-cropper.js > dist/angular-img-cropper.js",
    "js:min": "uglifyjs dist/angular-img-cropper.js > dist/angular-img-cropper.min.js",
    "js:copy:dev": "cp dist/angular-img-cropper.min.js example/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:IzumiSy/angular-img-cropper.git"
  },
  "auther": "Allan Bishop <allan.bishop@gmail.com>",
  "contributor": [
    {
      "name": "IzumiSy (IzumiSy)"
    },
    {
      "name": "Stas (SpeedShiter)"
    }
  ],
  "bugs": {
    "url": "https://github.com/IzumiSy/angular-img-cropper/issues"
  },
  "keywords": [
    "angular",
    "cropper",
    "image",
    "img",
    "rectangle",
    "aspect",
    "crop"
  ],
  "devDependencies": {
    "browserify": "^13.1.1",
    "jslint": "^0.10.3",
    "live-server": "^1.1.0",
    "uglifyjs": "^2.4.10"
  },
  "license": "MIT"
}
